<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>

<body>
    <h1>Posts</h1>
    <ol>
        <% results.forEach(function(post) {%>
        <li>
            <%= post.message %>
            <img src="<%= post.imageUrl %>" />
        </li>
        <% }); %>ejs
    </ol>
    <div id="navigations">
        <%if (previous) { %>
        <a href="<%= previous %>">Previous</a>
        <% } %>
        <%if (next) { %>
        <a href="<%= next %>">Next</a>
        <% } %>
    </div>
</body>
<%- include('_footer') -%>

</html>